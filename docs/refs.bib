@article{SELLIER20111461,
title = {An iterative method for the inverse elasto-static problem},
journal = {Journal of Fluids and Structures},
volume = {27},
number = {8},
pages = {1461-1470},
year = {2011},
issn = {0889-9746},
doi = {https://doi.org/10.1016/j.jfluidstructs.2011.08.002},
url = {https://www.sciencedirect.com/science/article/pii/S088997461100123X},
author = {M. Sellier},
keywords = {Inverse Problem, Optimal Shape Design},
abstract = {This brief communication describes the use of an iterative algorithm to tackle the inverse elasto-static problem which has two important applications: identifying the required initial body shape which deforms into a desired one when subject to a given set of loads; identifying the unloaded shape of a structure from a given deformed shape and a prescribed set of loads. The iterative algorithm stems from the rather intuitive observation that a better estimate of the required initial shape can be obtained by adding a correction to the previous shape estimate equal to the computed mismatch in the deformed shapes. A detailed convergence analysis explains the reason behind the success of the proposed algorithm. Compared to other techniques described in the literature to solve such inverse problems, this iterative scheme offers the advantage of being easy to implement and readily compatible with the use of commercial solver packages. Two benchmark problems are treated to illustrate the success of the algorithm: that of a punctured membrane and that of a cantilever beam in cross flow. The latter illustrates that the concepts described here readily applies to steady fluidâ€“structure interaction problems leading to an interesting range of new applications.}
}
